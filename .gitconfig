[remote "fastai"]
    url = git@github.com:fastai/course-v3.git
    fetch = +refs/heads/*:refs/remotes/fastai/*
[remote "origin"]
    url = git@github.com:huangyingw/fastai/course-v3.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = fastai
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[vrun]
	silent = false
	async = false
[deploy]
	host = localhost
	path = ~/myproject/git/AI/fastai/course-v3
